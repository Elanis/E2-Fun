        ActivateChevromNumbers = "111000011"
        timer("56",2650)
    }
        if (clk("56")) {
        ActivateChevromNumbers = "111100011"
        timer("57",2650)
    }
        if (clk("57")) {
        ActivateChevromNumbers = "111110011"
        timer("58",2650)
    }
        if (clk("58")) {
        ActivateChevromNumbers = "111110011"
        Open=1
    }    
}
else {
    ActivateChevromNumbers = "000000000"
    Started = 0
    Ring=0
    Open=0
}    
