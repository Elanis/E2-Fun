@name Chevron v2
@inputs Start 
@outputs ActivateChevromNumbers:string Ring Open Destination:string Vitesse Classic
@persist Started

############################################################
########################## Par Elanis ######################
################### https://github.com/Elanis ##############
############################################################

#Cette E2 Permet d'activer virtuellement 45 chevrons a la porte du Film ( Stargate Movie ) du CAP.

if(first()|dupefinished()) {
        Destination="EARTHP"
        Vitesse=2
        Classic=1
    }

interval(750)
if (Start == 1) {
    if (Started == 0) {
        timer("0",750)
        Started = 1
        Ring = 1
        Classic = 1
    }
    if (clk("0")) {
        ActivateChevromNumbers = "100000000"
        timer("1",750)
    }
        if (clk("1")) {
        ActivateChevromNumbers = "110000000"
        timer("2",750)
    }
        if (clk("2")) {
        ActivateChevromNumbers = "111000000"
        timer("3",750)
    }
        if (clk("3")) {
        ActivateChevromNumbers = "111000010"
        timer("4",750)
    }
        if (clk("4")) {
        ActivateChevromNumbers = "111000011"
        timer("5",750)
    }
        if (clk("5")) {
        ActivateChevromNumbers = "111100011"
        timer("6",750)
    }
        if (clk("6")) {
        ActivateChevromNumbers = "111110011"
        timer("7",750)
    }
        if (clk("7")) {
        ActivateChevromNumbers = "111110011"
        timer("8",750)
    }
        if (clk("8")) {
        ActivateChevromNumbers = "111111011"
        timer("9",750)
    }
        if (clk("9")) {
        ActivateChevromNumbers = "111111111"
        timer("10",500)
    }
        if (clk("10")) {
        Classic=0
        timer("10b",250)
    }
        if(clk("10b")) {
        ActivateChevromNumbers = "200000000"
        timer("10c",250)
    }
        if(clk("10c"))
    {
        Classic=1
        timer("11",500)
    }
        if (clk("11")) {
        ActivateChevromNumbers = "110000000"
        timer("12",750)
    }
        if (clk("12")) {
        ActivateChevromNumbers = "111000000"
        timer("13",750)
    }
        if (clk("13")) {
        ActivateChevromNumbers = "111000010"
        timer("14",750)
    }
